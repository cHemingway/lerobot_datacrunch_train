# Environment Configuration for Datacrunch LeRobot Training
# Copy this file to .env and fill in your actual values

# Datacrunch API credentials
DATACRUNCH_CLIENT_ID=your_datacrunch_client_id_here
DATACRUNCH_CLIENT_SECRET=your_datacrunch_client_secret_here

# ML Platform tokens
HUGGINGFACE_TOKEN=your_huggingface_token_here
WANDB_TOKEN=your_wandb_token_here

# Instance configuration
PRICE_CAP=1.0
REQUIRED_GPU=H100
IMAGE_NAME=ubuntu-24.04-cuda-12.8-open

# SSH configuration (required for secure authentication)
# Path to your SSH private key file. If not specified, will search common locations:
# ~/.ssh/id_rsa, ~/.ssh/id_ed25519, ~/.ssh/id_ecdsa, ~/.ssh/datacrunch, ~/.ssh/datacrunch_rsa
# SSH_KEY_PATH=/path/to/your/ssh/private/key
